package tasks;

public class Palindrome {
public static void main(String[] args) {
	int rangeStart = 100;
	int rangeEnd = 1000;
	for(int i = rangeStart; i < rangeEnd; i++) {
		int n, rev = 0, rem = 0;
		n = i;
		while(n > 0) {
			rem = n % 10;
			rev = rev * 10 + rem;
			n /= 10;
		}
		if(n == rev) {
			System.out.print(n + " ");
		}
	}
	
}
}
